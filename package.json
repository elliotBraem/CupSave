{
  "name": "cupsave",
  "version": "0.0.1",
  "private": true,
  "main": "./node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "jest -u --watchAll",
    "testWithCoverage": "jest --verbose --updateSnapshot --watch --coverage && open ./coverage/lcov-report/index.html",
    "reinit": "watchman watch-del-all && rm -rf node_modules package-lock.json && npm i && npm run clearcache",
    "clearcache": "rm -rf /tmp/metro-bundler-cache-* /tmp/haste-map-react-native-packager-*",
    "lint": "eslint --cache --ext .js,.jsx --fix \"./src/\"",
    "format": "prettier --write \"**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue,yaml,yml}\""
  },
  "rnpm": {
    "assets": [
      "src/assets/fonts"
    ]
  },
  "dependencies": {
    "expo": "^32.0.5",
    "firebase": "^5.9.1",
    "husky": "^1.3.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-maps": "^0.24.0",
    "react-native-svg-transformer": "^0.12.1",
    "react-navigation": "^3.5.1",
    "react-redux": "^6.0.0",
    "react-redux-firebase": "^3.0.0-alpha.10",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.6",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-firestore": "^0.7.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "expo-cli": "^2.10.1",
    "jest-expo": "^32.0.0",
    "prettier": "^1.17.0",
    "schedule": "0.4.0"
  },
  "jest": {
    "preset": "jest-expo"
  }
}
